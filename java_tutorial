// Nested if ststmentent loop
import java.util.*;
public class Autil {
	public static void main(String[] yashi) {
		
		Scanner obj = new Scanner(System.in);
		int a,b,c;
		System.out.println("Enter the number");
		a = obj.nextInt();
		System.out.println("Enter the second number");
		b = obj.nextInt();
		System.out.println("Enter the third number");
		c = obj.nextInt();
		if(a>b) {
			if(a>c) {
				System.out.println("Greater value_a = " + a);
			}else {
				System.out.println("Greater value_c = " + c);
			}
		}else {
			if(b>c) {
				System.out.println("Greater value_b = " + b);
			}else {
				System.out.println("Greater value_c = "+ c);
			}
		}
	}

}


// salary input 10000 increge salary in 10% Bonus and less 5% bonus;

import java.util.*;
public class Bonus {
	public static void main(String[] yashi) {
		int salary;
		int bonus;
		Scanner input = new Scanner(System.in);
		System.out.println("Enter the number ");
		salary = input.nextInt();
		if(salary > 10000) {
			bonus = (salary*10)/100;
			salary = salary+bonus;
		}
		else {
			bonus =(salary*5)/100;
			salary = salary+bonus;
			
		}
		
		System.out.println("Final salary " + salary);
	}

}


//A boolean data type is declared with the boolean keyword and can only take the values true or false:
public class Bool {
	public static void main(String[] args) {
		boolean T = true;
		boolean F = false;
		System.out.println("check condiction 'Ture and False' = " + T);
		System.out.println("check condiction 'Ture and False' = " + F);
	}

}
//Boolean values are mostly used for conditional testing, which you will learn more about in a later chapter.

//The byte data type can store whole numbers from -128 to 127. 
//This can be used instead of int or other integer types to save memory
//when you are certain that the value will be within -128 and 127:
public class Byte {
	public static void main(String[] yashi) {
		byte myNum = 100;
		System.out.println("The byte number in check " + myNum);
		
	}

}



public class Cbw {
	public static void main(String[] yashi) {
		int x=0;
		while (x < 10) {
			System.out.println("\n"+x);
			x++;
			if(x==5) {
				break;
			}
		}
		
		int y=0;
		while(y < 10) {
			if(y == 5) {
				y++;
				continue;
			}
			System.out.println(y);
			y++;
		}
	}

}

