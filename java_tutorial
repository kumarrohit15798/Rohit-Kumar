// for loop condiction
public class Forloop {
	public static void main(String[] args) {

		for(int i=1; i < 11; i++ ) {
			System.out.println(i);
		}
		
		for(int j=0; j <= 10; j=j+2) {
			System.out.println("even number = " +j);
		}
		for(int x = 1; x <= 11; x = x+2) {
			System.out.println("odd number = "+ x);
		}
	}

}


// for example * print
public class Labeled {
	public static void main(String[] yashi) {
		int i,j;
		for(i=1; i <= 5; i++) {
			for(j=1; j <= 5; j++) {
				System.out.println("*");
			}
			System.out.println(" ");
		}
		
		Loop1: for(i=0; i <= 10; i++) {
			System.out.println(" ");
			if (i == 5) {
				break;
			}
			for(j = 0; j <= 5; j++) {
				System.out.println("*");
				if(i==j) {
					continue Loop1;
				}
			}
		}
	}
	

}


//Break statement :- Jumping a part of a loop 
public class Lbreak {
	public static void main(String[] yashi) {
		int y;
		for(y=0; y<10; y++) {
			
			if(y==5) {
				break;
			}
			System.out.println("y_ value  " + y);
		}
// continue statement : Skipping one part of continue program	
		int i;
		for(i=0; i<10; i++) {
			if(i==5) {
				continue;
			}
			System.out.println("\n i _ value "+ i);
		}
	}

}

// logical operastion
public class Logical {
	public static void main(String[] args) {
		int x = 2;
		int y = 10;
		System.out.println(x < 5 &&  x < 10);
		System.out.println(x < 5 ||  x < 10);
		System.out.println (!(x < 5 &&  x < 10));
		
	}

}
// Mathmatics operater
public class Math {
	public static void main(String[] args) {
		int a = 10;
		
		int b = 20;
		
		System.out.println(a+b);
		System.out.println(b/a);
		System.out.println(a-b);
		System.out.println(a*b);
	}

}


// java math
public class Mathjava {
	public static void main(String[] args) {
		int a =15;
		int b = 10;
		
		
		// max function
		System.out.println(java.lang.Math.max(a,b));  
		
		// min function
		System.out.println(java.lang.Math.min(a,b));
		
		// The Math.sqrt(x) method returns the square root of x:
		System.out.println(java.lang.Math.sqrt(a));
		
		//The Math.abs(x) method returns the absolute (positive) value of x:
		
		System.out.println(java.lang.Math.abs(-4.8));
		
		//Math.random() returns a random number between 0 (inclusive), and 1 (exclusive):
		System.out.println(java.lang.Math.random());
	}

}


// Nested if statement in java
import java.util.*;
public class Nested {
	public static void main(String[] yashi) {
		Scanner obj = new Scanner(System.in);
		int a,b,c;
		 System.out.println("Enter the first number _A");
		 a = obj.nextInt();
		 System.out.println("Enter the Second number_B");
		 b = obj.nextInt();
		 System.out.println("Enter the third number_C");
		 c = obj.nextInt();
		 if(a>b) {
			 if(a>c) {
				 System.out.println("Greater value = " +a);
			 }else {
				 System.out.println("Greater value = " +c);
			 }
		 }if(b>c) {
			 System.out.println("Greater value = " +b);
		 }else {
			 System.out.println("Greater value = " +c);
		 }
		
	}

}

public class Operators {
	public static void main(String[] yashi) {
		int x = 10;
		int y = 5;
		int z = 3;
		//Adds together two values 
		System.out.println("Addistion number =" + (x+y));
		// Subtracts one value from another
		System.out.println("sub function ="+ (x-y));
		//Multiplies two values
		System.out.println("multipale number =" + (x*z));
		//Divides one value from another
		System.out.println("divide the number =" + (x/y));
		// Modulus, Returns the division remainder
		System.out.println("model method = " + (x%z));
		//Increment, Increases the value of a variable
		++x;
		System.out.println("Increment value = "+x);
		//Decrement, Decreases the value of a variable
		--y;
		System.out.println("Decrement value = " +y);
		
		
		
	}

}


// Boolean function
public class Sbool {
	public static void main(String[] args) {
		boolean yashi = true;
		boolean mangan = false;
		System.out.println(yashi);
		System.out.println(mangan);
		
		int x = 10;
		int  y= 20;
		System.out.println(x < y);
		
		System.out.println(x == 10);
		System.out.println(10 == 15); 
		
	}

}


//A floating point number can also be a scientific number with an "e" to indicate the power of 10:
public class Science {
	public static void main(String[] args) {
		float myNum= 12e3f;
		double myNumf = 12E3d;
		System.out.println("float function e(n^10) =" +myNum);
		System.out.println("double function E(n^10) =" + myNumf);
	}

}


// while loop
public class While {
	public static void main(String[] args) {
		int x =10;
		while(x < 21) {
			System.out.println("The x value = "+ x);
			x++;
		}
		
		int y = 5;
		do {
			System.out.println(y);
			y++;
		}
		while(y < 11);
	}

}


// if condistion
public class Yashi {
	public static void main(String[] yashi) {
		int x = 20;
		int y = 30;
		int time = 12;
		if (x < y) {
			System.out.println("x is greater than y ");
		}
		
		if (x < 10) {
			System.out.println("x is greater than 30");
		}else {
			
			System.out.print("X is not greater than 30\n");

		}
		
		if (time < 10) {
			System.out.println("good day");
		}else if(time < 11) {
			System.out.println("Good moring");
		}else {
			System.out.println("Good evening");
		}
		
		//Short Hand If...Else (Ternary Operator)
		int times = 20;
		
		String result;
		
		result = (times < 19) ? "Good day." : "Good moring.";
		System.out.println(result);
		
	}

}

