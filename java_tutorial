//The char data type is used to store a single character. 
//The character must be surrounded by single quotes, like 'A' or 'c':
public class Char {
	public static void main(String[] yashi) {
		char myChar = 'A';//letter used only single quotes
		System.out.println(myChar);
	}

}


public class Comparison {
	public static void main(String[] yashi) {
		int x = 10;
		int y = 15;
		int z = 3;
		System.out.println("Equal to" + (x==y));
		System.out.println("Not equal" + (y!=z));
		System.out.println("Gretar than = " +(x > y));
		System.out.println("less than = "+(x < y));
		System.out.println("gretar than equal "+(x>=y));
		System.out.println("less than equal " +(x <= z));
		
	}

}


// String Concatenation
public class Concatenation {
	public static void main(String[] yashi) {
		String firstName = "R.Mangan Maharaj";
		String secondName = "Yashi kesarwani";
		System.out.println(firstName + " Love " + secondName);
		//You can also use the concat() method to concatenate two strings:
		System.out.println(firstName.concat(secondName));
		
		//The sequence \"  inserts a double quote in a string:
		//String txt = "This is my "love" yashi kesarwani";
		String txt = "This is my \"love\"yashi kesarwani";
		System.out.println(txt);
		
		//The sequence \'  inserts a single quote in a string:
		String txt1 = "This is my loves\'s  yashi kesarwani";
		System.out.println(txt1);
		
		//The sequence \\  inserts a single backslash in a string:
		
		String txt2 = "This is my loves \\ yashi kesarwani";
		System.out.println(txt2);
		
		// new line
		String txt3 = "This is my love \n yashi kesarwani";
		System.out.println(txt3);
		
		// Carriage Return
		String txt4 = "This is my\r love  yashi kesarwani";
		System.out.println(txt4);
		
		//Tab
		String txt5 = "This is \t my love  yashi kesarwani";
		System.out.println(txt5);
		
		// Backspace
		
		String txt6 = "This is my baby \b name yashi";
		System.out.println(txt6);
		
		///  \f 	Form Feed 	
		
		String txt7 = "This is my \f baby name yashi";
		System.out.println(txt7);
		
		
		// Adding Numbers and Strings
		
		String x = "20";
		String y = "20";
		System.out.println(x+y);
		
		String z = "20";
		int a = 20;
		String q = a + z;
		System.out.println(q);
		
	
	}

}

// 1 to 100 even number print
public class Do {
	public static void main(String[] yashi) {
		int no = 0;
		do {
			no = no+2;
			System.out.println("Even number = " +no);	
		}
		while(no < 100);
	}

}
// 1 to 100 even number print
public class Do {
	public static void main(String[] yashi) {
		int no = 0;
		do {
			no = no+2;
			System.out.println("Even number = " +no);	
		}
		while(no < 100);
	}

}

// Student grade function on marks; 

import java.util.*;
public class Else {
	public static void main(String[] yashi) {
		int marks;
		char grade;
		Scanner input = new Scanner(System.in);
		System.out.println("Enter the number ");
		marks = input.nextInt();
		if(marks >= 85) {
			grade = 'A';
		}
		else if (marks >= 75) {
			grade = 'B';
		}
		else if(marks >= 65) {
			grade = 'C';
			
		}else {
			grade = 'D';
		}
		System.out.println("grade of student = " + grade);
	}

}


